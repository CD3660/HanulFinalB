<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="prod">
	<select id="list" resultType="prodVO">
		select *
		from product
	</select>
	<select id="info" resultType="prodVO">
		select * 
		from product 
		where prod_id = ${id}
	</select>
</mapper>


